<!DOCTYPE html>
<html lang="Ar" dir="rtl">
  <head>
    <%-include('../includes/head.ejs')%>
    <link rel="stylesheet" href="/css/earnings.css" />
  </head>
  <body>
    <%-include('../includes/nav.ejs')%>
    <main class="container">
      <div class="typo">
        <h1 class="typo__heading">مراقبة الاصناف</h1>
      </div>
      <%if (products.length > 0){%>
      <table>
        <tr>
          <th>اسم البائع</th>
          <th>اسم المنتج</th>
          <th>تاريخ الانشاء</th>
          <th>تعديل سعر الشحن</th>
        </tr>
        <%for (let p of products){%>
        <tr>
          <td><%=p.sellerId.name%></td>
          <td><%=p.title%></td>
          <td><%=p.dateAdded%></td>
          <td>
            <%=p.shippingFees%><a href="/designer/editDesign/<%=p._id%>">
              تعديل</a
            >
          </td>
        </tr>
        <%}%>
      </table>
      <%}else{%>
      <h2 class="notfound">لا يوجد منتجات جديده</h2>
      <%}%>
    </main>
    <%-include('../includes/end.ejs')%>
    <script src="/js/designActions.js"></script>
  </body>
</html>
